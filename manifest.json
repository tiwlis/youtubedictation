{
	"manifest_version": 2,
	"name": "Youtube Dictation",
	"description": "Youtube Dictation",
	"version": "1.0.0",
	"content_scripts": [{
		"matches": ["*://www.youtube.com/watch?v=*",
					"*://youtube.com/watch?v=*"],
		"css": ["styles/styles.css"],
		"js": ["js/jquery.js", "js/dictation.js"],
		"run_at": "document_end"
	}]
}